{
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": true,
  "all": true,
  "include": ["src/**/*.[tj]s?(x)"],
  "exclude": ["src/main.ts"],
  "reporter": ["html", "lcov", "json-summary"],
  "report-dir": "./coverage",
  "lines": 75,
  "functions": 75,
  "statements": 75,
  "branches": 75
}
